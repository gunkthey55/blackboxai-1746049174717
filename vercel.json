{
  "version": 2,
  "builds": [
    {
      "src": "server/index-static.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "components/**",
      "use": "@vercel/static"
    },
    {
      "src": "shared/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index-static.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "server/index-static.js"
    }
  ]
}
